{
  "terminal.integrated.profiles.windows": {
    "Next Front-end": {
      "path": "PowerShell",
      "args": [
        "-NoExit",
        "-Command",
        "cd '${workspaceFolder}/next-front-end'; pnpm dev --turbo"
      ]
    },
    "Nestjs Back-end": {
      "path": "PowerShell",
      "args": [
        "-NoExit",
        "-Command",
        "cd '${workspaceFolder}/nestjs-back-end'; pnpm start:dev"
      ]
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell"
}
